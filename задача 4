#include <locale.h>
#include <stdio.h>

void main()
{
	setlocale(LC_ALL, "RUS");
	puts("   __   __     __   ___    __  __    ");
	puts("   __| |__    |  |    |   |  |   |   ");
	puts("  |__  |__|.  |__|    | . |__|   |   ");
	getchar();
}
void date()
{
	setlocale(LC_ALL, "RUS");
